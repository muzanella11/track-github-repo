{
  "name": "track-github-repo",
  "version": "1.0.0",
  "description": "Track github repo",
  "author": "Nurfirliana Muzanella",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ext .js,.vue . --fix",
    "lint:style": "stylelint 'assets/scss/**/*css'",
    "lint:style:fix": "stylelint 'assets/scss/**/*css' --fix",
    "lint": "npm-run-all --parallel lint:js lint:style",
    "lint:fix": "npm-run-all --parallel lint:*:fix",
    "test": "jest"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.0.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "devDependencies": {
    "@mdi/font": "^4.9.95",
    "@mdi/js": "^4.9.95",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "jest": "^24.1.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "nuxtjs-mdi-font": "^1.0.2",
    "sass-loader": "^8.0.2",
    "store": "^2.0.12",
    "stylelint": "^13.2.0",
    "vue-jest": "^4.0.0-0",
    "vue-style-loader": "^4.1.2"
